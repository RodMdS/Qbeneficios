        <div class="dashboard-wrapper">
            <div class="container-fluid  dashboard-content">
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h2 class="pageheader-title">Atualizar Cadastro de Colaborador</h2>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Formulário</a></li>
                                        <li class="breadcrumb-item active" aria-current="page">Atualizar Cadastro de Colaborador</li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                    <div class="row">
                        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                            <div class="card">
                                <h5 class="card-header">Buscar o Colaborador pelo Nome</h5>
                                <div class="card-body">
                                    <form class="needs-validation" action="/search?tag=upd" method="POST">
                                        <div class="row">
                                            <div class="col-xl-9 col-lg-9 col-md-12 col-sm-12 col-12 mb-2">
                                                <label for="name">Nome do Colaborador</label>
                                                <input type="text" class="form-control" name="name" id="name" placeholder="Ex.: João" required>
                                            </div>

                                            <div class="col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12 mb-2" style="margin-top: 30px;">
                                                <button class="btn btn-success" type="submit">Buscar</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div> 

                    <div class="row">
                        <% if(list.length > 0) { %>
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                <table class="table table-hover">
                                    <thead class="table-danger">
                                      <tr>
                                        <th scope="col">ID</th>
                                        <th scope="col">NOME</th>
                                        <th scope="col">LOCAL DE NASCIMENTO</th>
                                        <th scope="col">DATA DE NASCIMENTO</th>
                                        <th scope="col">CEP</th>
                                        <th scope="col">CIDADE</th>
                                        <th scope="col">RG</th>
                                        <th scope="col">AÇÃO</th>
                                      </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i = 0; i < list.length; i++) { %>
                                            <tr>
                                                <th scope="row"><%= i + 1 %></th>
                                                <td><%= list[i].name %></td>
                                                <td><%= list[i].birthplace %></td>
                                                <td><%= list[i].date_of_birth %></td>
                                                <td><%= list[i].cep %></td>
                                                <td><%= list[i].city %></td>
                                                <td><%= list[i].rg %></td>
                                                <td><a href="/update_coll?id=<%= list[i]._id %>">ATUALIZAR</a></td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                            </div>
                        <% } %>
                    </div>
                            
            </div>
